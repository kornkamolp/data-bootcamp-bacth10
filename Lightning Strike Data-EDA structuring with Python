    "# group data by quarter\n",
    "df_by_quarter = union_df.groupby(['quarter']).sum().reset_index()\n",
    "df_by_quarter['number_of_strikes_formatted'] = df_by_quarter['number_of_strikes'].div(1000000).round(1).astype(str) + 'M'\n",
